AAA,GAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA,CAAA,SAAA;AACA,KANA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KATA,EAHA,CAaA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA;AACA;AACA,IAAA,aAHA,yBAGA,IAHA,EAGA;AACA,MAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA,CADA,CACA;;AACA,aACA,CAAA,MAAA,IAAA,CAAA,OAAA,EAAA,EAAA,KAAA,CAAA,CAAA,CAAA,IACA,GADA,GAEA,CAAA,OAAA,IAAA,CAAA,QAAA,KAAA,CAAA,CAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAFA,GAGA,GAHA,GAIA,IAAA,CAAA,WAAA,EALA;AAOA,KAZA;AAaA,IAAA,gBAbA,4BAaA,MAbA,EAaA;AACA,UAAA,UAAA,GAAA,EAAA;;AACA,cAAA,MAAA;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,EAAA,CAAA,UAAA,CAAA,OAAA;AACA;;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,EAAA,CAAA,UAAA,CAAA,OAAA;AACA;;AACA,aAAA,CAAA;AACA,UAAA,UAAA,GAAA,QAAA,CAAA,EAAA,CAAA,UAAA,CAAA,OAAA;AACA;;AACA;AACA,UAAA,UAAA,GAAA,EAAA;AACA;AAZA;;AAcA,aAAA,UAAA;AACA,KA9BA;AA+BA,IAAA,WA/BA,uBA+BA,MA/BA,EA+BA;AACA,aAAA,MAAA,GAAA,IAAA,IAAA,CAAA,YAAA,CAAA,OAAA,EAAA;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,QAAA,EAAA;AAFA,OAAA,EAGA,MAHA,CAGA,MAHA,CAAA;AAIA;AApCA,GAtCA;AA4EA,EAAA,OAAA,EAAA;AACA;AACA;AACA;AACA,IAAA,WAJA,uBAIA,QAJA,EAIA;AACA,WAAA,QAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,cAAA,CAAA,cAAA,CAAA,QAAA;AACA,KAPA;;AAQA;AACA;AACA;AACA,IAAA,mBAXA,iCAWA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAbA;;AAcA;AACA;AACA;AACA,IAAA,WAjBA,uBAiBA,UAjBA,EAiBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,KADA;AAAA;AAAA,uBAEA,KAAA,CACA,GADA,qDACA,UADA,GAEA,IAFA,CAEA,UAAA,OAAA,EAAA;AACA,kBAAA,EAAA,CAAA,YAAA,GAAA,OAAA,CAAA,IAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,OAAA,CAAA,IAAA;AACA,iBALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,iBARA,CAFA;;AAAA;AAWA,gBAAA,EAAA,CAAA,WAAA,CAAA,CAAA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AA7BA;AA5EA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"container-content-header\">\r\n      <div class=\"title\">Danh sách nhân viên</div>\r\n      <div class=\"container-content-header-btn\">\r\n        <BaseButton\r\n        id=\"btnDeleted\"\r\n        buttonText=\"Xóa nhân viên\"\r\n        />\r\n        <BaseButton\r\n          id=\"btnAdd\"\r\n          buttonText=\"Thêm Nhân viên\"\r\n          @btn-click=\"showAddForm(0)\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <div class=\"container-content-filter\">\r\n      <div class=\"container-content-filter-left\">\r\n        <div class=\"filter-search\">\r\n          <input\r\n            class=\"field-input\"\r\n            type=\"text\"\r\n            placeholder=\"Tìm theo mã, Tên hoặc Số điện thoại\"\r\n          />\r\n        </div>\r\n        <div class=\"filter-department\">\r\n          <div id=\"cbbPosition\" class=\"m-combobox\" style=\"width: 200px\">\r\n            <input type=\"text\" class=\"m-combobox-input\" value=\"Phòng ban\" />\r\n            <button class=\"m-combobox-button\">\r\n              <div class=\"arrow\"></div>\r\n            </button>\r\n            <div class=\"m-combobox-select d-none\">\r\n              <div class=\"m-combobox-item\" value=\"1\">Kinh doanh</div>\r\n              <div class=\"m-combobox-item\" value=\"2\">Nhân sự</div>\r\n              <div class=\"m-combobox-item m-item-selected\" value=\"3\">\r\n                Phát triển\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"filter-position\">\r\n          <div class=\"m-combobox\" style=\"width: 200px\">\r\n            <input type=\"text\" class=\"m-combobox-input\" value=\"Vị trí\" />\r\n            <button class=\"m-combobox-button\">\r\n              <div class=\"arrow\"></div>\r\n            </button>\r\n            <div class=\"m-combobox-select d-none\">\r\n              <div class=\"m-combobox-item\">Giám đốc</div>\r\n              <div class=\"m-combobox-item\">Trưởng phòng</div>\r\n              <div class=\"m-combobox-item m-item-selected\">Nhân viên</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"container-content-filter-right\"></div>\r\n    </div>\r\n    <div class=\"container-table-employee\">\r\n      <table id=\"tbListData\">\r\n        <thead>\r\n          <tr>\r\n            <th>\r\n              <div class=\"checkbox\"></div>\r\n            </th>\r\n            <th>Mã nhân viên</th>\r\n            <th>Họ và tên</th>\r\n            <th>Giới tính</th>\r\n            <th>Ngày sinh</th>\r\n            <th>Điện thoại</th>\r\n            <th>Email</th>\r\n            <th>Chức vụ</th>\r\n            <th>Phòng ban</th>\r\n            <th>Mức lương cơ bản</th>\r\n            <th>Tình trạng công việc</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            v-for=\"employee in employees\"\r\n            :key=\"employee.EmployeeId\"\r\n            @dblclick =\"dbOnClickTR(employee.EmployeeId)\"\r\n          >\r\n            <td><div class=\"checkbox\"></div></td>\r\n            <td>{{ employee.EmployeeCode }}</td>\r\n            <td>{{ employee.FullName }}</td>\r\n            <td>{{ employee.GenderName }}</td>\r\n            <td>{{ employee.DateOfBirth | FormatDateDMY }}</td>\r\n            <td>{{ employee.PhoneNumber }}</td>\r\n            <td>{{ employee.Email }}</td>\r\n            <td>{{ employee.PositionName }}</td>\r\n            <td>{{ employee.DepartmentName }}</td>\r\n            <td>{{ employee.Salary | FormatMoney }}</td>\r\n            <td>{{ employee.WorkStatus | FormatWorkStatus }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <div class=\"container-pages\">\r\n      <div class=\"record-captions\">Hiển thị 1-10/1000 nhân viên</div>\r\n      <div class=\"page-list\">\r\n        <div class=\"btn first-page\"></div>\r\n        <div class=\"btn prev-page\"></div>\r\n        <div class=\"btn page-number\">1</div>\r\n        <div class=\"btn page-number\">2</div>\r\n        <div class=\"btn page-number\">3</div>\r\n        <div class=\"btn page-number\">4</div>\r\n        <div class=\"btn next-page\"></div>\r\n        <div class=\"btn last-page\"></div>\r\n      </div>\r\n      <div class=\"number-record\">10 nhân viên/trang</div>\r\n    </div>\r\n    <EmployeeDialog\r\n      :isHidden=\"isHidden\"\r\n      @closeEmployeeDialog=\"closeEmployeeDialog\"\r\n      :employeeEdit = \"employeeEdit\"\r\n      ref=\"EmployeeDialog\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Resource from \"../../scripts/Resources\";\r\nimport EmployeeDialog from \"../employee/EmployeeDialog.vue\";\r\nimport BaseButton from \"../../components/Base/BaseButton.vue\";\r\nexport default {\r\n  name: \"EmployeePage\",\r\n  components: {\r\n    EmployeeDialog,\r\n    BaseButton,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      //form props\r\n      isHidden: true,\r\n      employeeEdit:{},\r\n      employees: [],\r\n      employee: {},\r\n      firstName: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    var me = this;\r\n    // Gọi API lấy dữ liệu:\r\n    axios\r\n      .get(\"http://cukcuk.manhnv.net/api/v1/Employees\")\r\n      .then(function (res) {\r\n        console.log(res);\r\n        me.employees = res.data;\r\n        console.log(me.employees);\r\n      })\r\n      .catch(function (res) {\r\n        console.log(res);\r\n      });\r\n    // console.log(\r\n    //   \"2. created (trước khi gán lại data):\" + this.employee.CustomerCode\r\n    // );\r\n    // this.employee = { CustomerCode: \"NV0001\" };\r\n    // console.log(\r\n    //   \"3. created (sau khi gán lại data):\" + this.employee.CustomerCode\r\n    // );\r\n  },\r\n  filters: {\r\n    //Chỉnh lại tên Hàm trong js chuẩn convention\r\n    // format bằng Vue Date Picker \r\n    FormatDateDMY(date) {\r\n      date = new Date(date);//Khởi tạo biến chuẩn \r\n      return (\r\n        (\"0\" + date.getDate()).slice(-2) +\r\n        \"/\" +\r\n        (\"0\" + (date.getMonth() + 1)).slice(-2) +\r\n        \"/\" +\r\n        date.getFullYear()\r\n      );\r\n    },\r\n    FormatWorkStatus(status) {\r\n      var workStatus = \"\";\r\n      switch (status) {\r\n        case 0:\r\n          workStatus = Resource.VN.WorkStatus.Testing;\r\n          break;\r\n        case 1:\r\n          workStatus = Resource.VN.WorkStatus.Working;\r\n          break;\r\n        case 2:\r\n          workStatus = Resource.VN.WorkStatus.Quitted;\r\n          break;\r\n        default:\r\n          workStatus = \"\";\r\n          break;\r\n      }\r\n      return workStatus;\r\n    },\r\n    FormatMoney(salary) {\r\n      return (salary = new Intl.NumberFormat(\"vi-VI\", {\r\n        style: \"currency\",\r\n        currency: \"VND\",\r\n      }).format(salary));\r\n    },\r\n  },\r\n  methods: {\r\n    /**\r\n     * Method show Dialog\r\n     */\r\n    showAddForm(formMode) {\r\n      this.isHidden = false;\r\n      this.$refs.EmployeeDialog.changeFormMode(formMode);\r\n    },\r\n    /**\r\n     * Method ẩn Dialog\r\n     */\r\n    closeEmployeeDialog(){\r\n      this.isHidden = true;\r\n    },\r\n    /**\r\n     * Sự kiện dbOnClickTR\r\n     */\r\n    async dbOnClickTR(EmployeeId){\r\n      let me = this\r\n      await axios\r\n      .get(`http://cukcuk.manhnv.net/api/v1/Employees/${EmployeeId}`)\r\n      .then((respone)=>{\r\n        me.employeeEdit = respone.data;\r\n        console.log(respone.data)\r\n      })\r\n      .catch((error)=>{\r\n        console.log(error);\r\n      })\r\n      me.showAddForm(1);\r\n    }\r\n\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import url(\"../../styles/Layout/container-content.css\");\r\n</style>"],"sourceRoot":"src/views/employee"}]}