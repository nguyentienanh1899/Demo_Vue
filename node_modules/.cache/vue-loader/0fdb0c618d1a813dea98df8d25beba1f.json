      <div class=\"row\">\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"true\"\r\n                  labelText=\"Số CCCD/CMND\"\r\n                  v-model=\"employee.IdentityNumber\"\r\n                  :originValue=\"employee.IdentityNumber\"\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"false\"\r\n                  labelText=\"Ngày cấp\"\r\n                  v-model=\"employee.IdentityDate\"\r\n                  :originValue=\"employee.IdentityDate\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"false\"\r\n                  labelText=\"Nơi cấp\"\r\n                  v-model=\"employee.IdentityPlace\"\r\n                  :originValue=\"employee.IdentityPlace\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"true\"\r\n                  labelText=\"Email\"\r\n                  v-model=\"employee.Email\"\r\n                  :originValue=\"employee.Email\"\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"true\"\r\n                  labelText=\"Số điện thoại\"\r\n                  v-model=\"employee.PhoneNumber\"\r\n                  :originValue=\"employee.PhoneNumber\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"B\">\r\n            <div>B.THÔNG TIN CÔNG VIỆC</div>\r\n            <div class=\"line\"></div>\r\n            <div class=\"row\">\r\n              <div class=\"col\">\r\n                <label for=\"\">Phòng ban</label><br />\r\n                <BaseComboBox\r\n                  itemId=\"DepartmentId\"\r\n                  itemText=\"DepartmentName\"\r\n                  myUrl=\"http://cukcuk.manhnv.net/api/v1/Departments\"\r\n                  v-model=\"employee.DepartmentId\"\r\n                  :originId=\"mployee.DepartmentId + ''\"\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <label for=\"\">Vị trí</label><br />\r\n                <BaseComboBox\r\n                  itemId=\"PositionId\"\r\n                  itemText=\"PositionName\"\r\n                  myUrl=\"http://cukcuk.manhnv.net/api/v1/Positions\"\r\n                  v-model=\"employee.PositionId\"\r\n                  :originId=\"mployee.PositionId + ''\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"false\"\r\n                  labelText=\"Mã số thuế\"\r\n                  v-model=\"employee.PersonalTaxCode\"\r\n                  :originValue=\"employee.PersonalTaxCode\"\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"false\"\r\n                  labelText=\"Mức lương\"\r\n                  v-model=\"employee.Salary\"\r\n                  :originValue=\"employee.Salary\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col\">\r\n                <BaseFieldInput\r\n                  :isRequired=\"false\"\r\n                  labelText=\"Ngày gia nhập công ty\"\r\n                  v-model=\"employee.CreatedDate\"\r\n                  :originValue=\"employee.CreatedDate\"\r\n                />\r\n              </div>\r\n              <div class=\"col\">\r\n                <label for=\"\">Tình trạng công việc</label><br />\r\n                <BaseComboBox\r\n                  itemId=\"WorkStatus\"\r\n                  itemText=\"WorkStatusName\"\r\n                  v-model=\"employee.WorkStatus\"\r\n                  :originId=\"employee.WorkStatus + ''\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"container-dialog-footer\">\r\n        <button class=\"button save\" id=\"btnSave\" @click=\"onClickBtnSave\">\r\n          Lưu\r\n        </button>\r\n        <button class=\"button create\">Thêm</button>\r\n        <button class=\"button cancel\" id=\"btnCancel\" @click=\"btnCloseOnClick\">\r\n          Hủy\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BaseFieldInput from \"../../components/Base/BaseFieldInput.vue\";\r\nimport BaseComboBox from \"../../components/Base/BaseCombobox.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"EmployeeDialog\",\r\n  components: {\r\n    BaseComboBox,\r\n    BaseFieldInput,\r\n  },\r\n  props: {\r\n    isHidden: Boolean,\r\n    employeeEdit: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      employee: {},\r\n      formMode: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    btnCloseOnClick() {\r\n      this.$emit(\"closeEmployeeDialog\");\r\n    },\r\n    changeFormMode(formMode) {\r\n      this.formMode = formMode;\r\n      this.employee = this.employeeEdit;\r\n      console.log(\"EMployee\", this.employee);\r\n    },\r\n    async onClickBtnSave() {\r\n      if (this.formMode == 0) {\r\n        axios\r\n          .post(\"http://cukcuk.manhnv.net/api/v1/Employees\", this.employee)\r\n          .then((respone) => {\r\n            console.log(respone);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      } else if (this.formMode == 1) {\r\n        await axios\r\n          .put(\r\n            `http://cukcuk.manhnv.net/api/v1/Employees/${this.employee.EmployeeId}`,\r\n            this.employee\r\n          )\r\n          .then((respone) => {\r\n            console.log(respone);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import url(\"../../styles/Component/dialog.css\");\r\n</style>"]}]}