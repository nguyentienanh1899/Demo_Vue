{"remainingRequest":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\src\\components\\layout\\TheContent.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\src\\components\\layout\\TheContent.vue","mtime":1635676171295},{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\css-loader\\dist\\cjs.js","mtime":1635681448118},{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1635681450634},{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\postcss-loader\\src\\index.js","mtime":1635681448427},{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1635681448612},{"path":"C:\\Users\\tienanh\\Desktop\\Demo-Vue\\demo_vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1635681448989}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["TheContent.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAqKA","file":"TheContent.vue","sourceRoot":"src/components/layout","sourcesContent":["<template lang=\"\">\r\n     <div class=\"container-content\">\r\n        <div class=\"container-content-header\">\r\n            <div class=\"title\">Danh sách nhân viên</div>\r\n            <div class=\"container-content-header-btn\">\r\n                <div class=\"button\" id=\"btnDeleted\">\r\n                    <div class=\"button-icon\"></div>\r\n                    <div class=\"button-text\">Xóa nhân viên</div>\r\n                </div>\r\n                <div class=\"button\" id=\"btnAdd\">\r\n                    <div class=\"button-icon\"></div>\r\n                    <div class=\"button-text\">Thêm nhân viên</div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"container-content-filter\">\r\n            <div class=\"container-content-filter-left\">\r\n                <div class=\"filter-search\">\r\n                    <input class=\"field-input\" type=\"text\" placeholder=\"Tìm theo mã, Tên hoặc Số điện thoại\" />\r\n                </div>\r\n                <div class=\"filter-department\">\r\n                    <div id=\"cbbPosition\" class=\"m-combobox\" style=\"width: 200px;\">\r\n                        <input type=\"text\" class=\"m-combobox-input\" value=\"Phòng ban\">\r\n                        <button class=\"m-combobox-button\">\r\n                            <div class=\"arrow\"></div>\r\n                        </button>\r\n                        <div class=\"m-combobox-select\">\r\n                            <div class=\"m-combobox-item\" value=\"1\">Kinh doanh</div>\r\n                            <div class=\"m-combobox-item\" value=\"2\">Nhân sự</div>\r\n                            <div class=\"m-combobox-item m-item-selected\" value=\"3\">Phát triển</div>\r\n                        </div>\r\n                     </div>\r\n                </div>\r\n                <div class=\"filter-position\">\r\n                    <div class=\"m-combobox\" style=\"width: 200px;\">\r\n                        <input type=\"text\" class=\"m-combobox-input\" value=\"Vị trí\">\r\n                        <button class=\"m-combobox-button\">\r\n                            <div class=\"arrow\"></div>\r\n                        </button>\r\n                        <div class=\"m-combobox-select\">\r\n                            <div class=\"m-combobox-item\">Giám đốc</div>\r\n                            <div class=\"m-combobox-item\">Trưởng phòng</div>\r\n                            <div class=\"m-combobox-item m-item-selected\">Nhân viên</div>\r\n                        </div>\r\n                     </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"container-content-filter-right\">\r\n            </div>\r\n        </div>\r\n        <div class=\"container-table-employee\">\r\n            <table id=\"tbListData\">\r\n                <thead>\r\n                    <tr>\r\n                        <th >\r\n                            <div class='checkbox'></div>\r\n                        </th>\r\n                        <th>Mã nhân viên</th>\r\n                        <th>Họ và tên</th>\r\n                        <th>Giới tính</th>\r\n                        <th>Ngày sinh</th>\r\n                        <th>Điện thoại</th>\r\n                        <th>Email</th>\r\n                        <th>Chức vụ</th>\r\n                        <th>Phòng ban</th>\r\n                        <th>Mức lương cơ bản</th>\r\n                        <th>Tình trạng công việc</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"customer in customers\" :key=\"customer.CustomerId\">\r\n                        <td><div class='checkbox'></div></td>\r\n                        <td>{{customer.EmployeeCode}}</td>\r\n                        <td>{{customer.FullName}}</td>\r\n                        <td>{{customer.GenderName}}</td>\r\n                        <td>{{customer.DateOfBirth|FormatDateDMY}}</td>\r\n                        <td>{{customer.PhoneNumber}}</td>\r\n                        <td>{{customer.Email}}</td>\r\n                        <td>{{customer.PositionName}}</td>\r\n                        <td>{{customer.DepartmentName}}</td>\r\n                        <td>{{customer.Salary| FormatMoney}}</td>\r\n                        <td>{{customer.WorkStatus|FormatWorkStatus}}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n        <div class=\"container-pages\">\r\n            <div class=\"record-captions\">Hiển thị 1-10/1000 nhân viên</div>\r\n            <div class=\"page-list\">\r\n                <div class=\"btn first-page\"></div>\r\n                <div class=\"btn prev-page\"></div>\r\n                <div class=\"btn page-number\">1</div>\r\n                <div class=\"btn page-number\">2</div>\r\n                <div class=\"btn page-number\">3</div>\r\n                <div class=\"btn page-number\">4</div>\r\n                <div class=\"btn next-page\"></div>\r\n                <div class=\"btn last-page\"></div>\r\n            </div>\r\n            <div class=\"number-record\">10 nhân viên/trang</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport Resource from \"../../scripts/Resources\"\r\nexport default {\r\n    name:\"Content\",\r\n     data() {\r\n    return {\r\n      customers: [],\r\n      customer: {},\r\n      firstName: \"\",\r\n    };\r\n  },\r\n    created() {\r\n      var me = this;\r\n    // Gọi API lấy dữ liệu:\r\n    axios\r\n      .get(\"http://cukcuk.manhnv.net/api/v1/Employees\")\r\n      .then(function (res) {\r\n        console.log(res);\r\n        me.customers = res.data;\r\n        console.log(me.customers);\r\n      })\r\n      .catch(function (res) {\r\n        console.log(res);\r\n      });\r\n    // console.log(\r\n    //   \"2. created (trước khi gán lại data):\" + this.customer.CustomerCode\r\n    // );\r\n    // this.customer = { CustomerCode: \"NV0001\" };\r\n    // console.log(\r\n    //   \"3. created (sau khi gán lại data):\" + this.customer.CustomerCode\r\n    // );\r\n    },\r\n    filters:{\r\n        FormatDateDMY(date) {\r\n            date = new Date(date);\r\n            return (\"0\" + date.getDate()).slice(-2) + '/' + (\"0\" + (date.getMonth() + 1)).slice(-2) + '/' + date.getFullYear();\r\n        },\r\n        FormatWorkStatus(status) {\r\n        var workStatus = \"\"\r\n        switch (status) {\r\n            case 0:\r\n                workStatus = Resource.VN.WorkStatus.Testing\r\n                break\r\n            case 1:\r\n                workStatus = Resource.VN.WorkStatus.Working\r\n                break\r\n            case 2:\r\n                workStatus = Resource.VN.WorkStatus.Quitted\r\n                break\r\n            default:\r\n                workStatus = \"\"\r\n                break\r\n            }\r\n            return workStatus;\r\n        },\r\n        FormatMoney(salary) {\r\n            return salary = new Intl.NumberFormat('vi-VI', { style: 'currency', currency: 'VND' }).format(salary);\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style>\r\n     @import url('../../styles/Layout/container-content.css');\r\n</style>"]}]}